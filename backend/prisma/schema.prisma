generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model student {
  id        Int      @id @default(autoincrement())
  Name      String
  Cohort    String
  Courses   String
  JoinedAt  DateTime @default(now())
  LastLogin DateTime @default(now()) // Automatically set to the current time
}
